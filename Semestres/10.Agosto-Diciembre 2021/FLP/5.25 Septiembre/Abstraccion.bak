#lang eopl
;;Implementaci√≥n
(define zero 0)
(define is-zero?
   (lambda (n) (= n 0)))
(define successor
  (lambda (n) (+ n 1)))
(define predecessor
  (lambda (n) (if (is-zero? n)
                  (eopl:error "Cero no tiene predecesor" "predecessor")
                  (- n 1))))

;;Area del programador
(define plus
  (lambda (a b)
    (cond
      [(is-zero? b) a]
      [else
       (successor (plus a (predecessor b)))]
      )
    ))

(plus 70 80)